<?xml version="1.0" encoding="UTF-8"?>
<project name="module_ludumdare22" default="compile.module.ludumdare22">
  <dirname property="module.ludumdare22.basedir" file="${ant.file.module_ludumdare22}"/>
  
  <property name="module.jdk.home.ludumdare22" value=""/>
  <property name="module.jdk.bin.ludumdare22" value=""/>
  <property name="module.jdk.classpath.ludumdare22" value=""/>
  
  <property name="compiler.args.ludumdare22" value="${compiler.args}"/>
  
  <property name="ludumdare22.output.dir" value="${module.ludumdare22.basedir}/out/production/LudumDare22"/>
  <property name="ludumdare22.testoutput.dir" value="${module.ludumdare22.basedir}/out/test/LudumDare22"/>
  
  <path id="ludumdare22.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="ludumdare22.module.production.classpath"/>
  
  <path id="ludumdare22.runtime.production.module.classpath">
    <pathelement location="${ludumdare22.output.dir}"/>
  </path>
  
  <path id="ludumdare22.module.classpath">
    <pathelement location="${ludumdare22.output.dir}"/>
  </path>
  
  <path id="ludumdare22.runtime.module.classpath">
    <pathelement location="${ludumdare22.testoutput.dir}"/>
    <pathelement location="${ludumdare22.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.ludumdare22">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.ludumdare22">
    <patternset refid="excluded.from.module.ludumdare22"/>
  </patternset>
  
  
  <target name="compile.module.ludumdare22" depends="compile.module.ludumdare22.production,compile.module.ludumdare22.tests" description="Compile module LudumDare22"/>
  
  <target name="compile.module.ludumdare22.production" depends="register.custom.compilers" description="Compile module LudumDare22; production classes"/>
  
  <target name="compile.module.ludumdare22.tests" depends="register.custom.compilers,compile.module.ludumdare22.production" description="compile module LudumDare22; test classes" unless="skip.tests"/>
  
  <target name="clean.module.ludumdare22" description="cleanup module">
    <delete dir="${ludumdare22.output.dir}"/>
    <delete dir="${ludumdare22.testoutput.dir}"/>
  </target>
</project>